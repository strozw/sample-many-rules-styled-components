{
  "private": true,
  "workspaces": [
    "workspaces/*"
  ],
  "devDependencies": {
    "cross-env": "7.0.2",
    "customize-cra": "1.0.0",
    "npm-run-all": "4.1.5",
    "react-app-rewired": "2.1.6",
    "wsrun": "5.2.1",
    "yarn": "1.22.5"
  },
  "dependencies": {
    "styled-components": "5.2.0"
  },
  "scripts": {
    "install:sc:fixed-insert-rules": "cd ./workspaces/fixed-insert-rules/styled-components && yarn && yarn build",
    "install:sc:force-enabled-speedy": "cd ./workspaces/force-enabled-speedy/styled-components && yarn && yarn build",
    "install:sc": "run-p install:sc:*",
    "start": "wsrun -a start"
  }
}
